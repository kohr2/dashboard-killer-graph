{
  "entities": {
    "Company": {
      "id": "Company",
      "name": "Company",
      "description": "A business organization or corporation",
      "properties": {
        "name": { "type": "string", "description": "Company name" },
        "ticker": { "type": "string", "description": "Stock ticker symbol" },
        "cik": { "type": "string", "description": "Central Index Key (SEC identifier)" },
        "founded": { "type": "date", "description": "Company founding date" },
        "headquarters": { "type": "string", "description": "Company headquarters location" },
        "website": { "type": "string", "description": "Company website URL" },
        "description": { "type": "text", "description": "Company description" },
        "marketCap": { "type": "number", "description": "Market capitalization" },
        "employees": { "type": "number", "description": "Number of employees" },
        "revenue": { "type": "number", "description": "Annual revenue" },
        "lei": { "type": "string", "description": "Legal Entity Identifier" }
      }
    },
    "Industry": {
      "id": "Industry",
      "name": "Industry",
      "description": "A specific business industry or sector",
      "properties": {
        "name": { "type": "string", "description": "Industry name" },
        "code": { "type": "string", "description": "Industry classification code" },
        "description": { "type": "text", "description": "Industry description" },
        "parentIndustry": { "type": "string", "description": "Parent industry if applicable" }
      }
    },
    "Sector": {
      "id": "Sector",
      "name": "Sector",
      "description": "A broad business sector category",
      "properties": {
        "name": { "type": "string", "description": "Sector name" },
        "code": { "type": "string", "description": "Sector classification code" },
        "description": { "type": "text", "description": "Sector description" }
      }
    },
    "BusinessType": {
      "id": "BusinessType",
      "name": "BusinessType",
      "description": "Type of business organization",
      "properties": {
        "name": { "type": "string", "description": "Business type name" },
        "description": { "type": "text", "description": "Business type description" },
        "legalStructure": { "type": "string", "description": "Legal structure type" }
      }
    },
    "CompanySize": {
      "id": "CompanySize",
      "name": "CompanySize",
      "description": "Classification of company size",
      "properties": {
        "name": { "type": "string", "description": "Size category name" },
        "description": { "type": "text", "description": "Size category description" },
        "employeeRange": { "type": "string", "description": "Employee count range" },
        "revenueRange": { "type": "string", "description": "Revenue range" }
      }
    },
    "StockExchange": {
      "id": "StockExchange",
      "name": "StockExchange",
      "description": "Stock exchange where company is listed",
      "properties": {
        "name": { "type": "string", "description": "Exchange name" },
        "code": { "type": "string", "description": "Exchange code" },
        "country": { "type": "string", "description": "Exchange country" },
        "description": { "type": "text", "description": "Exchange description" }
      }
    }
  },
  "relationships": {
    "BELONGS_TO_INDUSTRY": {
      "id": "BELONGS_TO_INDUSTRY",
      "name": "BELONGS_TO_INDUSTRY",
      "description": "Company belongs to a specific industry",
      "sourceEntity": "Company",
      "targetEntity": "Industry",
      "properties": {
        "primary": { "type": "boolean", "description": "Whether this is the primary industry" },
        "percentage": { "type": "number", "description": "Percentage of business in this industry" }
      }
    },
    "BELONGS_TO_SECTOR": {
      "id": "BELONGS_TO_SECTOR",
      "name": "BELONGS_TO_SECTOR",
      "description": "Company belongs to a specific sector",
      "sourceEntity": "Company",
      "targetEntity": "Sector",
      "properties": {
        "primary": { "type": "boolean", "description": "Whether this is the primary sector" }
      }
    },
    "HAS_BUSINESS_TYPE": {
      "id": "HAS_BUSINESS_TYPE",
      "name": "HAS_BUSINESS_TYPE",
      "description": "Company has a specific business type",
      "sourceEntity": "Company",
      "targetEntity": "BusinessType",
      "properties": {}
    },
    "HAS_SIZE": {
      "id": "HAS_SIZE",
      "name": "HAS_SIZE",
      "description": "Company has a specific size classification",
      "sourceEntity": "Company",
      "targetEntity": "CompanySize",
      "properties": {
        "asOfDate": { "type": "date", "description": "Date of size classification" }
      }
    },
    "LISTED_ON": {
      "id": "LISTED_ON",
      "name": "LISTED_ON",
      "description": "Company is listed on a stock exchange",
      "sourceEntity": "Company",
      "targetEntity": "StockExchange",
      "properties": {
        "ticker": { "type": "string", "description": "Ticker symbol on this exchange" },
        "listingDate": { "type": "date", "description": "Date of listing" },
        "active": { "type": "boolean", "description": "Whether listing is currently active" }
      }
    },
    "SUBSIDIARY_OF": {
      "id": "SUBSIDIARY_OF",
      "name": "SUBSIDIARY_OF",
      "description": "Company is a subsidiary of another company",
      "sourceEntity": "Company",
      "targetEntity": "Company",
      "properties": {
        "ownershipPercentage": { "type": "number", "description": "Percentage ownership" },
        "relationshipType": { "type": "string", "description": "Type of subsidiary relationship" }
      }
    },
    "COMPETES_WITH": {
      "id": "COMPETES_WITH",
      "name": "COMPETES_WITH",
      "description": "Company competes with another company",
      "sourceEntity": "Company",
      "targetEntity": "Company",
      "properties": {
        "competitionLevel": { "type": "string", "description": "Level of competition" },
        "marketOverlap": { "type": "number", "description": "Market overlap percentage" }
      }
    },
    "INDUSTRY_BELONGS_TO_SECTOR": {
      "id": "INDUSTRY_BELONGS_TO_SECTOR",
      "name": "INDUSTRY_BELONGS_TO_SECTOR",
      "description": "Industry belongs to a sector",
      "sourceEntity": "Industry",
      "targetEntity": "Sector",
      "properties": {}
    }
  },
  "entityExtractionPatterns": {
    "Company": [
      {
        "name": "company_name_with_ticker",
        "pattern": "\\b([A-Z][a-zA-Z\\s&.,]+)\\s*\\(([A-Z]{1,5})\\)",
        "description": "Company name followed by ticker symbol in parentheses"
      },
      {
        "name": "ticker_symbol",
        "pattern": "\\b[A-Z]{1,5}\\b",
        "description": "Standalone ticker symbol (3-5 capital letters)"
      },
      {
        "name": "company_name_capitalized",
        "pattern": "\\b[A-Z][a-zA-Z\\s&.,]{2,50}\\b",
        "description": "Capitalized company names"
      },
      {
        "name": "inc_corp_llc",
        "pattern": "\\b[A-Z][a-zA-Z\\s&.,]+\\s+(Inc|Corp|LLC|Ltd|Company|Co)\\b",
        "description": "Company names ending with business suffixes"
      }
    ],
    "Industry": [
      {
        "name": "industry_terms",
        "pattern": "\\b(Technology|Healthcare|Finance|Manufacturing|Retail|Energy|Transportation|Telecommunications|Media|Real Estate|Agriculture|Mining|Construction|Education|Entertainment|Food|Automotive|Aerospace|Pharmaceuticals|Biotechnology)\\b",
        "description": "Common industry terms"
      }
    ],
    "Sector": [
      {
        "name": "sector_terms",
        "pattern": "\\b(Technology|Healthcare|Financial|Consumer|Industrial|Energy|Materials|Utilities|Communication Services|Real Estate)\\b",
        "description": "Common sector terms"
      }
    ]
  },
  "reasoning": {
    "algorithms": {
      "company_industry_classification": {
        "name": "Company Industry Classification",
        "description": "Classifies companies into industries based on their business description and keywords",
        "entityType": "Company",
        "factors": ["description", "keywords", "businessActivities"],
        "weights": [0.4, 0.3, 0.3],
        "threshold": 0.7,
        "relationshipType": "BELONGS_TO_INDUSTRY"
      },
      "market_cap_classification": {
        "name": "Market Cap Classification",
        "description": "Classifies companies by size based on market capitalization",
        "entityType": "Company",
        "factors": ["marketCap"],
        "weights": [1.0],
        "threshold": 0.8,
        "relationshipType": "HAS_SIZE"
      }
    }
  }
} 