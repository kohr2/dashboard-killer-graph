{
  "name": "ProcurementOntology",
  "source": "https://eprocurementontology.github.io/",
  "dependencies": [
    "crm"
  ],
  "entities": {
    "ProcurementProcedure": {
      "description": "A series of activities leading to the conclusion of a public contract. Based on ePO.",
      "documentation": "https://eprocurementontology.github.io/"
    },
    "Contract": {
      "description": "The awarded contract resulting from a procurement procedure. Based on ePO.",
      "documentation": "https://eprocurementontology.github.io/"
    },
    "Tender": {
      "description": "A formal invitation to bid for a contract",
      "properties": {
        "title": {
          "type": "string",
          "description": "Tender title"
        },
        "value": {
          "type": "number",
          "description": "Estimated contract value"
        },
        "deadline": {
          "type": "date",
          "description": "Submission deadline"
        },
        "category": {
          "type": "string",
          "description": "Procurement category"
        }
      },
      "documentation": "https://eprocurementontology.github.io/"
    },
    "ProcuringEntity": {
      "description": "The organization conducting the procurement procedure. Based on ePO.",
      "parent": "Organization",
      "documentation": "https://eprocurementontology.github.io/"
    },
    "EconomicOperator": {
      "description": "An organisation that can be a tenderer or sub-contractor. Based on ePO.",
      "parent": "Organization",
      "documentation": "https://eprocurementontology.github.io/"
    },
    "Lot": {
      "description": "A subdivision of a tender",
      "properties": {
        "name": {
          "type": "string",
          "description": "Lot name"
        },
        "description": {
          "type": "string",
          "description": "Lot description"
        },
        "value": {
          "type": "number",
          "description": "Lot value"
        },
        "category": {
          "type": "string",
          "description": "Lot category"
        }
      },
      "documentation": "https://eprocurementontology.github.io/"
    },
    "Criterion": {
      "description": "A condition used for evaluation in a procurement procedure (e.g., award, selection, exclusion).",
      "properties": {
        "id": {
          "type": "string",
          "isId": true,
          "description": "Unique identifier for the criterion."
        },
        "name": {
          "type": "string",
          "description": "The name or title of the criterion."
        },
        "description": {
          "type": "string",
          "description": "A detailed description of the criterion."
        },
        "type": {
          "type": "string",
          "description": "The type of criterion (e.g., Award, Selection, Exclusion)."
        },
        "weight": {
          "type": "number",
          "description": "The weight of the criterion in the evaluation process, if applicable."
        }
      },
      "documentation": "https://eprocurementontology.github.io/"
    }
  },
  "relationships": {
    "CONTAINS": {
      "domain": "Tender",
      "range": "Lot",
      "description": "Tender contains lots",
      "documentation": "https://eprocurementontology.github.io/"
    }
  },
  "reasoning": {
    "algorithms": {
      "lot_similarity": {
        "name": "lot_similarity",
        "description": "Calculate similarity between lots based on category and value",
        "entityType": "Lot",
        "factors": [
          "category",
          "value"
        ],
        "weights": [
          0.6,
          0.4
        ],
        "threshold": 0.6,
        "relationshipType": "LOT_SIMILARITY"
      },
      "supplier_matching": {
        "name": "supplier_matching",
        "description": "Match suppliers to lots based on specialization",
        "entityType": "Supplier",
        "pattern": "e.specialization = lot.category",
        "patternName": "specialization_match",
        "relationshipType": "MATCHES_SPECIALIZATION"
      },
      "tender_risk": {
        "name": "risk_assessment",
        "description": "Assess tender risk based on deadline and value",
        "entityType": "Tender",
        "factors": [
          "deadline_proximity",
          "value_complexity"
        ],
        "weights": [
          0.4,
          0.6
        ],
        "threshold": 0.5
      }
    }
  },
  "advancedRelationships": {
    "LOT_SIMILARITY": {
      "domain": "Lot",
      "range": "Lot",
      "description": "Similarity relationship between lots based on category and value",
      "properties": {
        "score": {
          "type": "float",
          "description": "Similarity score between 0 and 1"
        },
        "factors": {
          "type": "array",
          "description": "List of factors used in similarity calculation"
        },
        "calculatedAt": {
          "type": "datetime",
          "description": "When the similarity was calculated"
        },
        "algorithm": {
          "type": "string",
          "description": "Algorithm used for similarity calculation"
        },
        "metadata": {
          "type": "object",
          "description": "Additional metadata about the similarity"
        }
      },
      "enabled": true,
      "algorithms": [
        {
          "name": "LOT_SIMILARITY",
          "description": "Calculate similarity between lots based on category and value",
          "entityType": "Lot",
          "factors": [
            {
              "property": "category",
              "weight": 0.6,
              "type": "categorical"
            },
            {
              "property": "value",
              "weight": 0.4,
              "type": "numeric"
            }
          ],
          "threshold": 0.6,
          "metadata": {
            "algorithm": "weighted_property_similarity",
            "source": "procurement_analysis"
          }
        }
      ],
      "customQuery": "MATCH (lot1:Lot)-[r:LOT_SIMILARITY]->(lot2:Lot) WHERE r.score > 0.6 RETURN lot1.name as lot1Name, lot2.name as lot2Name, r.score as similarity ORDER BY r.score DESC"
    },
    "temporal": {
      "enabled": false,
      "patterns": []
    },
    "hierarchical": {
      "enabled": false,
      "structures": []
    },
    "similarity": {
      "enabled": true,
      "algorithms": [
        {
          "name": "LOT_SIMILARITY",
          "description": "Calculate similarity between lots based on category and value",
          "entityType": "Lot",
          "factors": [
            {
              "property": "category",
              "weight": 0.6,
              "type": "categorical"
            },
            {
              "property": "value",
              "weight": 0.4,
              "type": "numeric"
            }
          ],
          "threshold": 0.6,
          "metadata": {
            "algorithm": "weighted_property_similarity",
            "source": "procurement_analysis"
          }
        }
      ]
    },
    "complex": {
      "enabled": false,
      "patterns": []
    },
    "queries": {
      "timeline": {
        "enabled": false,
        "customQuery": ""
      },
      "hierarchy": {
        "enabled": false,
        "customQuery": ""
      },
      "similarity": {
        "enabled": true,
        "customQuery": "MATCH (lot1:Lot)-[r:LOT_SIMILARITY]->(lot2:Lot) WHERE r.score > 0.6 RETURN lot1.name as lot1Name, lot2.name as lot2Name, r.score as similarity ORDER BY r.score DESC"
      },
      "complex": {
        "enabled": false,
        "customQuery": ""
      }
    }
  },
  "entityExtraction": {
    "models": {},
    "contextRules": {},
    "enrichment": {}
  }
}